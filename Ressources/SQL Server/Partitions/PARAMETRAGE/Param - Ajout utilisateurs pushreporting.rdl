    <DataSet Name="Dts_Groupe">
      <Query>
        <DataSourceName>Ds_BotanicDW_MEC</DataSourceName>
        <CommandText>select idGroupe, StrGroupe
from push_reporting.groupe
order by StrGroupe</CommandText>
      </Query>
      <Fields>
        <Field Name="idGroupe">
          <DataField>idGroupe</DataField>
