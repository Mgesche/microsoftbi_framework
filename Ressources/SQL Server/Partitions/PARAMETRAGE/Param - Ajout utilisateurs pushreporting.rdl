    <DataSet Name="Dts_User_Enabled_Default">
      <Query>
        <DataSourceName>Ds_BotanicDW_MEC</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@idUser">
            <Value>=Parameters!P_User.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>IF NOT EXISTS(select Enabled 
from push_reporting.[user] 
where strMail = @idUser) BEGIN
SELECT cast(1 as bit) as Enabled
END ELSE  BEGIN
select cast(Enabled as bit) as Enabled
from push_reporting.[user] 
where strMail = @idUser
END</CommandText>
      </Query>
