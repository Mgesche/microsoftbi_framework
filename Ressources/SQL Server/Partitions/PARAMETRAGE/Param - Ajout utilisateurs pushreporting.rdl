    <DataSet Name="Dts_Groupe_Default">
      <Query>
        <DataSourceName>Ds_BotanicDW_MEC</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@idUser">
            <Value>=Parameters!P_User.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select GRP.idGroupe
from push_reporting.groupe GRP
JOIN push_reporting.rel_groupe_user REL
  ON REL.idGroupe = GRP.idGroupe
JOIN push_reporting.[user] USR
  ON REL.idUser = USR.idUser
WHERE USR.strMail = @idUser</CommandText>
      </Query>
      <Fields>
        <Field Name="idGroupe">
          <DataField>idGroupe</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
