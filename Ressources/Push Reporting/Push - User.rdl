  <DataSets>
    <DataSet Name="Dts_User">
      <Query>
        <DataSourceName>Ds_BotanicDW_MEC</DataSourceName>
        <CommandText>select -1 as idUser, '_ Nouveau' as StrMail, 1 as Enabled
union
select idUser, StrMail, Enabled
from push_reporting.[user]
order by StrMail</CommandText>
      </Query>
      <Fields>
        <Field Name="idUser">
          <DataField>idUser</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="strMail">
          <DataField>StrMail</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="enabled">
          <DataField>Enabled</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Dts_User_Enabled_Default">
      <Query>
        <DataSourceName>Ds_BotanicDW_MEC</DataSourceName>
        <CommandText>=iif(Parameters!P_User.Value=-1,
" select 1 as Enabled ",
" select Enabled "
+"from push_reporting.[user] "
+"where idUser = "+Parameters!P_User.Value)</CommandText>
      </Query>
      <Fields>
        <Field Name="Enabled">
          <DataField>Enabled</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Dts_User_Mail_Default">
